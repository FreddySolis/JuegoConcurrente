<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Label layoutX="21.0" layoutY="14.0" prefHeight="17.0" prefWidth="67.0" text="Server" />
            <TextArea fx:id="txtAreaEventLog" editable="false" layoutX="15.0" layoutY="270.0" prefHeight="92.0" prefWidth="611.0" wrapText="true" />
            <Label layoutX="14.0" layoutY="254.0" text="Log" />
            <ListView fx:id="listUsersConnected" layoutX="172.0" layoutY="61.0" prefHeight="186.0" prefWidth="298.0" />
            <Label layoutX="276.0" layoutY="44.0" text="Usuarios conectados" />
            <Button fx:id="btnStartServer" layoutX="215.0" layoutY="10.0" mnemonicParsing="false" onAction="#startServer" prefHeight="25.0" prefWidth="90.0" text="Iniciar" />
            <Button fx:id="btnStopServer" disable="true" layoutX="329.0" layoutY="10.0" mnemonicParsing="false" onAction="#stopServer" prefHeight="25.0" prefWidth="90.0" text="Detener" />
      </children>
    </AnchorPane>
  </children>
  <stylesheets>
    <URL value="@../../chat/view/css/BasicApplication.css" />
  </stylesheets>
</VBox>
